#!/usr/bin/zsh
[[ "$HOST" != "x1-carbon" ]] && exit 1

CERT=false
CMD=$(~/.local/bin/certcheck ~/.cert/nm-openvpn/zimpler-cert.pem)
if [ $? -eq 1 ]; then
    CERT="[ Zimpler CERT exp ]"
fi

if [ "$CERT" != "false" ]; then
    echo $CERT
else
    exit 0
fi

echo "" # short_text
echo "#fc2003" # color
echo "#000000" # background color

